{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-plusplus": "off",
      "import/extensions": ["error", "ignorePackages"],
      "no-underscore-dangle": "off",
      "no-param-reassign": "off"
    }
}
